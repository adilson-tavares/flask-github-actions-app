{
    "version": 2,
    "builds": [
      { "src": "api/api.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "/api/api.py"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index"
      }
    ]
  }