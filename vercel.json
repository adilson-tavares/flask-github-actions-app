{
    "version": 2,
    "builds": [
        {
            "src": "api/api.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/api.py"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}