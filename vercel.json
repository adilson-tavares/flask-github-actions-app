{
    "version": 2,
    "builds": [
        {
            "src": "api/api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "api/(.*)",
            "dest": "/api"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}